{
  "name": "VueJsStarterKit",
  "description": "A VueJS starter kit. I couldn't find any full examples, so I made one.",
  "author": "Arman Abbasi",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && gulp sass && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.3.2",
    "vue": "^2.0.1",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.0",
    "vue-server-renderer": "^2.1.8",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.16.0",
    "buble-loader": "^0.4.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^v2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-sass": "^3.1.0",
    "html-minifier-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^3.10.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "sw-precache-webpack-plugin": "^0.7.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.0",
    "vue-server-renderer": "^2.0.2",
    "vue-template-compiler": "^2.1.8",
    "webpack": "2.1.0-beta.26",
    "webpack-dev-server": "^2.1.0-beta.11",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-node-externals": "^1.5.4"
  }
